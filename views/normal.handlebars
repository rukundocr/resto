<h5>NORMAL STAFF HISTORY</h5>
{{#if message}}
<h4 class="red-text text-darken-4">{{message}}</h4>
{{/if}}


<div class="row">
    <div class="col s12">
        <table class="highlight" ">
            <thead>
                <tr>
                    <th>Time</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <td>CARD</td>
                    <td>NID</td>
                    <td>District</td>
                   
                    <td>Phone</td>
                    <th>Temp</th>
                    <th >Status</th>
                    <th>ACTION</th>
                </tr>
            </thead>

            <tbody>
                {{#each results as |result key|}}

                <tr>
                    <td>{{result.time}}</td>
                        <td>{{result.firstname}}</td>
                        <td>{{result.lastname}}</td>
                        <td>{{result.card}}</td>
                        <td>{{result.NID}}</td>
                        <td>{{result.District}}</td>
                        <td>{{result.Phone}}</td>
                    <td>{{result.temperature}}</td>
                    <td>{{result.status}}</td>
                    <td>


                        <a href="/normal/delete/{{result._id}}" type="button" class="btn pink accent-3">DELETE</a>


                    </td>
                </tr>
            </tbody>
            {{else}}

            <p>No users to display</p>

            {{/each}}



        </table>

    </div>

</div>
<script>
    $(document).ready(function () {
        $('#table1').DataTable({
        });
        $('select').formSelect();
    });
</script>